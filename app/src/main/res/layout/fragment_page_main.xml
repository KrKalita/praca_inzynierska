<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cos11"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainPageFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:orientation="vertical">
        <Space
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="30000" />

        <Button
            android:id="@+id/lista_produktow"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="50dp"
            android:layout_marginVertical="10dp"
            android:background="@drawable/change_background_button"
            android:fontFamily="monospace"
            android:text="Lista produktów"
            android:textColor="@color/white" />

        <Button
            android:id="@+id/lista_uzytkownikow"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="50dp"
            android:layout_marginVertical="10dp"
            android:background="@drawable/change_background_button"
            android:fontFamily="monospace"
            android:textColor="@color/white"
            android:text="lista użytkowników" />

        <Button
            android:id="@+id/dostepDoRejestracji"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="50dp"
            android:layout_marginVertical="10dp"
            android:background="@drawable/change_background_button"
            android:fontFamily="monospace"
            android:textColor="@color/white"
            android:text="Dostęp do rejestracji" />

        <Button
            android:id="@+id/lista_kosztow"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="50dp"
            android:layout_marginVertical="10dp"
            android:background="@drawable/change_background_button"
            android:fontFamily="monospace"
            android:textColor="@color/white"
            android:text="Lista kosztów" />

        <Button
            android:id="@+id/komunikatySM"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="50dp"
            android:layout_marginVertical="10dp"
            android:background="@drawable/change_background_button"
            android:fontFamily="monospace"
            android:text="Komunikaty o niskim stanie magazynowym"
            android:textColor="@color/white" />
        <Space
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="50000" />

    </LinearLayout>

</FrameLayout>